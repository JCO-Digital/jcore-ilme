{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 3,
  "settings": {
    "appearanceTools": true,
    "border": {},
    "color": {
      "defaultPalette": false,
      "custom": false,
      "defaultGradients": false,
      "customGradient": false,
      "defaultDuotone": false,
      "palette": [
        {
          "color": "#41c4ff",
          "name": "Brand",
          "slug": "brand"
        },
        {
          "color": "#ffffff",
          "name": "White",
          "slug": "white"
        },
        {
          "color": "#d2d2d2",
          "name": "Light",
          "slug": "light"
        },
        {
          "color": "#3b3b3b",
          "name": "Dark",
          "slug": "dark"
        },
        {
          "color": "#000000",
          "name": "Black",
          "slug": "black"
        },
        {
          "color": "#4bf60a",
          "name": "Success",
          "slug": "success"
        },
        {
          "color": "#f6ea0a",
          "name": "Warning",
          "slug": "warning"
        },
        {
          "color": "#f60a0a",
          "name": "Failure",
          "slug": "failure"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(to right, var(--wp--preset--color--white), var(--wp--preset--color--brand))",
          "name": "White to Brand",
          "slug": "white-to-brand"
        }
      ]
    },
    "custom": {
      "lineHeight": {
        "xs": "1.1",
        "sm": "1.2",
        "md": "1.4",
        "lg": "1.5"
      },
      "breakpoints": {
        "sm": "640px",
        "md": "768px",
        "lg": "1024px",
        "xl": "1280px",
        "2xl": "1536px"
      },
      "spacingSizes": {
        "small": "clamp(1rem, 3.7vw + -0.05rem,  2rem)",
        "medium": "clamp(1.5rem, 3.7vw + -0.05rem,  3rem )",
        "large": "clamp(2rem, 3.7vw + -0.05rem,  4rem )"
      }
    },
    "dimensions": {
      "minHeight": true
    },
    "layout": {
      "contentSize": "min(800px, 80vw)",
      "wideSize": "min(1200px, 95vw)"
    },
    "position": {},
    "shadow": {},
    "spacing": {
      "blockGap": null,
      "margin": true,
      "padding": true,
      "defaultSpacingSizes": false,
      "spacingSizes": [
        {
          "name": "Fluid small",
          "size": "var:custom|spacing-sizes|small",
          "slug": "fluid-small"
        },
        {
          "name": "Fluid medium",
          "size": "var:custom|spacing-sizes|medium",
          "slug": "fluid-medium"
        },
        {
          "name": "Fluid large",
          "size": "var:custom|spacing-sizes|large",
          "slug": "fluid-large"
        }
      ]
    },
    "typography": {
      "fluid": true,
      "lineHeight": false,
      "letterSpacing": false,
      "fontFamilies": [
        {
          "fontFace": [
            {
              "fontFamily": "Inter Tight",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "300 900",
              "src": ["file:./assets/fonts/InterTight.woff"]
            },
            {
              "fontFamily": "Inter Tight",
              "fontStretch": "normal",
              "fontStyle": "italic",
              "fontWeight": "300 900",
              "src": ["file:./assets/fonts/InterTight-Italic.woff"]
            }
          ],
          "fontFamily": "\"Inter Tight\", sans-serif",
          "name": "Inter Tight",
          "slug": "inter-tight"
        },
        {
          "fontFace": [
            {
              "fontFamily": "Inter",
              "fontStretch": "normal",
              "fontStyle": "normal",
              "fontWeight": "300 900",
              "src": ["file:./assets/fonts/Inter.woff2"]
            }
          ],
          "fontFamily": "\"Inter\", sans-serif",
          "name": "Inter",
          "slug": "inter"
        }
      ],
      "defaultFontSizes": false,
      "fontSizes": [
        {
          "fluid": false,
          "name": "Paragraph",
          "slug": "paragraph",
          "size": "16px"
        },
        {
          "name": "H1",
          "slug": "heading-1",
          "size": "42px"
        },
        {
          "name": "H2",
          "slug": "heading-2",
          "size": "36px"
        },
        {
          "name": "H3",
          "slug": "heading-3",
          "size": "24px"
        },
        {
          "name": "H4",
          "slug": "heading-4",
          "size": "20px"
        },
        {
          "name": "Ingress",
          "slug": "ingress",
          "size": "26px",
          "fluid": {
            "min": "20px",
            "max": "26px"
          }
        },
        {
          "fluid": false,
          "name": "Mini",
          "slug": "mini",
          "size": "13px"
        },
        {
          "name": "Maxi",
          "slug": "maxi",
          "size": "64px"
        }
      ]
    },
    "useRootPaddingAwareAlignments": false,
    "blocks": {
      "core/pullquote": {
        "border": {
          "color": true,
          "radius": true,
          "style": true,
          "width": true
        }
      },
      "core/image": {
        "lightbox": {
          "enabled": true,
          "allowEditing": true
        }
      }
    }
  },
  "styles": {
    "typography": {
      "fontFamily": "Inter"
    },
    "color": {
      "text": "var:preset|color|dark",
      "background": "var:preset|color|white"
    },
    "blocks": {
      "core/paragraph": {
        "typography": {
          "lineHeight": "var:custom|line-height|md",
          "fontSize": "var:preset|font-size|paragraph"
        }
      },
      "core/heading": {
        "typography": {
          "fontSize": "var:preset|font-size|heading-2"
        }
      },
      "core/group": {
        "spacing": {
          "padding": {
            "top": "var:custom|spacing-sizes|medium",
            "bottom": "var:custom|spacing-sizes|medium"
          }
        }
      }
    },
    "elements": {
      "heading": {
        "typography": {
          "fontFamily": "Inter Tight"
        }
      },
      "link": {
        "color": {
          "text": "var:preset|color|brand"
        }
      }
    }
  },
  "customTemplates": [],
  "templateParts": [],
  "patterns": []
}
